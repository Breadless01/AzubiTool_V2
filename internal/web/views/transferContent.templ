// bcs_data.templ
package views

import "AzubiTool/internal/domain"

templ TransferContent(errMsg string) {
    <div class="main-wrapper">
        <h1>BCS -> Blok</h1>
        if errMsg != "" {
            <div class="error-msg">{errMsg}</div>
        } 
        else {
            {ctx.Value("user").(*domain.User).Oid}
        }
    </div>
}
